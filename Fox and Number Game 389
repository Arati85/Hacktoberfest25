#include<bits/stdc++.h>
using namespace std;
int gcd(int a,int b){
    if(b==0) return a;
    return gcd(b,a%b);
}
int solve(int n,int arr[]){
    int h=arr[0];
    for(int i=0;i<n;i++){
         h= gcd(h,arr[i]);
    }
    return h*n;
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    cout<<solve(n,arr);
    return 0;
}
